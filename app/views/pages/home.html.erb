<!-- Mixpanel event tracking -->
<% if user_signed_in? %>
  <script>
    mixpanel.track("Homepage-Returning");

    mixpanel.identify("<%= current_user.id %>")
    mixpanel.people.set({
      "$name": "<%= current_user.name %>",
      "$email": "<%= current_user.email %>",
      "Paid": "<%= current_user.amount %>"
    })
  </script>
<% else %>
  <script>
    mixpanel.track("Homepage-New");
  </script>
<% end %>